\n        <el-form :inline=\"true\" :model=\"serachCondition\" ref=\"searchForm\">\r\n          <el-form-item prop=\"school\" label=\"学校\" class=\"marginRight\">\r\n            <el-input\r\n              v-model=\"serachCondition.school\"\r\n              placeholder=\"按学校搜索\"\r\n            ></el-input> </el-form-item\r\n          ><el-form-item prop=\"studentnumber\" label=\"学 / 工号\" class=\"marginRight\">\r\n            <el-input\r\n              v-model=\"serachCondition.studentnumber\"\r\n              placeholder=\"按学/工号搜索\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"name\" label=\"姓名\" class=\"marginRight\">\r\n            <el-input\r\n              v-model=\"serachCondition.name\"\r\n              placeholder=\"按姓名搜索\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"power\" label=\"身份\" class=\"marginRight\">\r\n            <el-select v-model=\"serachCondition.power\" placeholder=\"请选择\">\r\n              <el-option label=\"学生\" value=\"学生\"></el-option>\r\n              <el-option label=\"教师\" value=\"教师\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item class=\"marginRight\">\r\n            <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\r\n            <el-button @click=\"resetForm('searchForm')\">重置</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <!-- 表格区 -->\r\n      <div class=\"tableContent\">\r\n        <el-table :data=\"tableData\" stripe style=\"width: 100%\">\r\n          <el-table-column\r\n            property=\"school\"\r\n            label=\"学校\"\r\n            min-width=\"22%\"\r\n          ></el-table-column>\r\n          <el-table-column\r\n            property=\"studentnumber\"\r\n            label=\"学/工号\"\r\n            min-width=\"20%\"\r\n          ></el-table-column>\r\n          <el-table-column\r\n            property=\"name\"\r\n            label=\"姓名\"\r\n            min-width=\"20%\"\r\n          ></el-table-column>\r\n          <el-table-column\r\n            property=\"score\"\r\n            label=\"成绩\"\r\n            min-width=\"18%\"\r\n          ></el-table-column>\r\n          <el-table-column\r\n            property=\"scoredate\"\r\n            label=\"日期\"\r\n            min-width=\"20%\"\r\n          ></el-table-column>\r\n        </el-table>\r\n      </div>\r\n      <!-- 分页 -->\r\n      <div class=\"paginationContainer\">\r\n        <el-pagination\r\n          class=\"elPage\"\r\n          background\r\n          layout=\"prev, pager, next\"\r\n          :page-size=\"10\"\r\n          :total=\"total\"\r\n          :current-page=\"currentPage\"\r\n          @current-change=\"handleCurrentChange\"\r\n        >\r\n        </el-pagination>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getStorage } from \"../util/StorageMan\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      currentPage: 1,\r\n      total: 0,\r\n      serachCondition: {\r\n        school: getStorage(\"userInfo\") ? getStorage(\"userInfo\").school : \"\",\r\n        name: \"\",\r\n        studentnumber: \"\",\r\n        power: \"学生\",\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    // this.scoreContent();\r\n    this.scoreSearch();\r\n  },\r\n  methods: {\r\n    // 获取总条数\r\n    // scoreContent() {\r\n    //   this.$axios\r\n    //     .get(`/scoreContent`)\r\n    //     .then((res) => {\r\n    //       let { code, msg } = res.data;\r\n    //       if (code == 200) {\r\n    //         this.total = res.data.data;\r\n    //       } else {\r\n    //         this.$message({\r\n    //           showClose: true,\r\n    //           message: msg,\r\n    //           type: \"warning\",\r\n    //         });\r\n    //       }\r\n    //     })\r\n    //     .catch((err) => {\r\n    //       this.$message({\r\n    //         showClose: true,\r\n    //         message: \"获取总条数失败: \" + err,\r\n    //         type: \"warning\",\r\n    //       });\r\n    //     });\r\n    // },\r\n    scoreSearch() {\r\n      this.$axios\r\n        .get(`/allScore?currentPage=${this.currentPage}`, {\r\n          params: this.serachCondition,\r\n        })\r\n        .then((res) => {\r\n          let { code, msg } = res.data;\r\n          if (code == 200) {\r\n            this.tableData = res.data.data;\r\n            this.total = res.data.total;\r\n            // console.log(this.tableData,'===')\r\n          } else {\r\n            this.$message({\r\n              showClose: true,\r\n              message: msg,\r\n              type: \"warning\",\r\n            });\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          this.$message({\r\n            showClose: true,\r\n            message: \"获取数据失败: \" + err,\r\n            type: \"warning\",\r\n          });\r\n        });\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n      // 如果\r\n      this.scoreSearch();\r\n    },\r\n\r\n    onSubmit() {\r\n      this.currentPage = 1;\r\n      this.scoreSearch();\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields()\r\n      this.currentPage = 1;\r\n      this.scoreSearch();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.cardBox {\r\n  width: 100%;\r\n  height: 95%;\r\n  /* display: flex;\r\n  justify-content: center;\r\n  align-items: center; */\r\n  padding-top: 20px;\r\n}\r\n.box-table {\r\n  width: 95%;\r\n  min-height: 100%;\r\n  margin: 0 auto;\r\n\r\n  position: relative;\r\n}\r\n.tableContent {\r\n  padding-bottom: 25px;\r\n}\r\n.paginationContainer {\r\n  float: right;\r\n  position: absolute;\r\n  bottom: 10px;\r\n  right: 10px;\r\n}\r\n.demo-form-inline {\r\n  margin-top: 20px;\r\n}\r\n.marginRight {\r\n  margin-right: 20px !important;\r\n}\r\n</style>"]}]}