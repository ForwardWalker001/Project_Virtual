);\r\n    },\r\n    scoreSearch() {\r\n      this.$axios\r\n        .get(`/allScore?currentPage=${this.currentPage}`)\r\n        .then((res) => {\r\n          let { code, msg } = res.data;\r\n          if (code == 200) {\r\n            this.tableData = res.data.data;\r\n            // console.log(this.tableData,'===')\r\n          } else {\r\n            this.$message({\r\n              showClose: true,\r\n              message: msg,\r\n              type: \"warning\",\r\n            });\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          this.$message({\r\n            showClose: true,\r\n            message: \"获取数据失败: \" + err,\r\n            type: \"warning\",\r\n          });\r\n        });\r\n    },\r\n    handleCurrentChange (val) {\r\n      this.currentPage = val\r\n      this.scoreSearch()\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.cardBox {\r\n  width: 100%;\r\n  height: 100%;\r\n  /* padding: 10px 0; */\r\n  /* display: flex;\r\n  justify-content: center;\r\n  align-items: center; */\r\n  margin-top : 20px\r\n}\r\n.box-table {\r\n  width: 95%;\r\n  /* min-height: 95%; */\r\n  margin: auto;\r\n}\r\n\r\n.paginationContainer {\r\n  float: right;\r\n  padding-top: 15px;\r\n  padding-bottom: 15px;\r\n}\r\n</style>"]}]}